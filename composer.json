
{
    "name": "huseyinfiliz/flagunapproved",
    "description": "A Flarum extension that turns flagged posts into unapproved status.",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0"
    },
    "extra": {
        "flarum-extension": {
            "title": "Flag to Unapproved",
            "icon": {
                "name": "fas fa-flag",
                "backgroundColor": "#FF0000",
                "color": "#FFFFFF"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "FlagToUnapproved\\": "src/"
        }
    }
}
